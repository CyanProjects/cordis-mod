{
  "name": "cordis",
  "description": "Meta-Framework for Modern JavaScript Applications",
  "version": "4.0.0-alpha.0",
  "type": "module",
  "bin": "src/bin/cordis.ts",
  "main": "src/index.ts",
  "exports": {
