    "./cli": "./src/cli.ts",
    "./group": "./src/plugins/group.ts",
    "./import": "./src/plugins/import.ts",
    "./loader": "./src/plugins/loader.ts",
    "./worker": "./src/worker/index.ts",
    "./main": "./src/worker/main.ts",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/cordis.git",
